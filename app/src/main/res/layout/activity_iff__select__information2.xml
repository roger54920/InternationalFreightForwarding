<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/layout_ll_head_mm_bg">

    <include layout="@layout/iff_title_layout" />


    <LinearLayout
        style="@style/padding_left_right_13dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:background="@color/iff_FFFFFF"
        android:orientation="vertical"
        android:paddingBottom="10dp">

        <LinearLayout style="@style/mw_b3dp_t8dp_ll">

            <TextView
                style="@style/w60dp_w_666_15sp"
                android:text="提货地址" />

            <EditText
                android:id="@+id/selection_receiving_address_et"
                style="@style/mv_r75dp_l15dp_et"
                android:hint="请输入提货地址" />

            <Button
                style="@style/btn_blue__70dp_65_30dp"
                android:text="@string/choice"
                android:visibility="invisible" />
        </LinearLayout>

        <include layout="@layout/iff_line_view_layout" />

        <LinearLayout style="@style/mw_b3dp_t8dp_ll">

            <TextView
                style="@style/w60dp_w_666_15sp"
                android:text="货&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;值" />

            <EditText
                android:id="@+id/value_of_goods_et"
                style="@style/mv_r75dp_l15dp_et"
                android:hint="请输入货品价值"
                android:inputType="phone|number" />

            <EditText
                android:id="@+id/money_type"
                style="@style/btn_blue__70dp_65_30dp"
                android:background="@drawable/details_of_goods_bg_style"
                android:lines="1"
                android:text="RMB"
                android:gravity="center"
                android:textColor="@color/iff_666666"
                android:ellipsize="end"
                android:textColorHint="@color/iff_999999"
                android:textCursorDrawable="@drawable/edit_cursor_shape"
                android:textSize="15sp" />
        </LinearLayout>

        <include layout="@layout/iff_line_view_layout" />

        <LinearLayout style="@style/mw_b3dp_t8dp_ll">

            <TextView
                style="@style/w60dp_w_666_15sp"
                android:text="期望报价" />

            <EditText
                android:id="@+id/offer_et"
                style="@style/mv_r75dp_l15dp_et"
                android:hint="请输入期望报价"
                android:inputType="phone|number" />

            <TextView
                style="@style/w60dp_w_666_15sp"
                android:layout_marginLeft="-15dp"
                android:text="元" />

            <Button
                style="@style/btn_blue__70dp_65_30dp"
                android:text="@string/choice"
                android:visibility="invisible" />
        </LinearLayout>

        <include layout="@layout/iff_line_view_layout" />

        <LinearLayout style="@style/mw_b3dp_t8dp_ll">

            <TextView
                style="@style/w60dp_w_666_15sp"
                android:text="体积大小" />

            <EditText
                android:id="@+id/volume_size_et"
                style="@style/mv_r75dp_l15dp_et"
                android:hint="请输入体积大小"
                android:inputType="number|phone" />

            <Button
                style="@style/btn_blue__70dp_65_30dp"
                android:text="@string/choice"
                android:visibility="invisible" />
        </LinearLayout>

        <include layout="@layout/iff_line_view_layout" />

        <LinearLayout style="@style/mw_b3dp_t8dp_ll">

            <TextView
                style="@style/w60dp_w_666_15sp"
                android:text="生产厂家" />

            <EditText
                android:id="@+id/manufacturer_et"
                style="@style/mv_r75dp_l15dp_et"
                android:hint="请输入生产厂家" />

            <Button
                style="@style/btn_blue__70dp_65_30dp"
                android:text="@string/choice"
                android:visibility="invisible" />
        </LinearLayout>

        <include layout="@layout/iff_line_view_layout" />

        <LinearLayout style="@style/mw_b3dp_t8dp_ll">

            <TextView
                style="@style/w60dp_w_666_15sp"
                android:text="许可证号" />

            <EditText
                android:id="@+id/license_key_et"
                style="@style/mv_r75dp_l15dp_et"
                android:hint="请输入许可证号"
                android:inputType="phone|number" />

            <Button
                style="@style/btn_blue__70dp_65_30dp"
                android:text="@string/choice"
                android:visibility="invisible" />

        </LinearLayout>

        <include layout="@layout/iff_line_view_layout" />

        <LinearLayout style="@style/mw_b3dp_t8dp_ll">

            <TextView
                style="@style/w60dp_w_666_15sp"
                android:text="批准文号" />

            <EditText
                android:id="@+id/approval_number_et"
                style="@style/mv_r75dp_l15dp_et"
                android:hint="请输入批准文号" />

            <Button
                style="@style/btn_blue__70dp_65_30dp"
                android:text="@string/choice"
                android:visibility="invisible" />
        </LinearLayout>

        <include layout="@layout/iff_line_view_layout" />

        <LinearLayout style="@style/mw_b3dp_t8dp_ll">

            <TextView
                style="@style/w60dp_w_666_15sp"
                android:text="征免性质" />

            <EditText
                android:id="@+id/exempted_nature_et"
                style="@style/mv_r75dp_l15dp_et"
                android:hint="请输入征免性质" />

            <Button
                style="@style/btn_blue__70dp_65_30dp"
                android:text="@string/choice"
                android:visibility="invisible" />
        </LinearLayout>

        <include layout="@layout/iff_line_view_layout" />


    </LinearLayout>

    <Button
        android:id="@+id/complete_btn"
        android:layout_width="255dp"
        android:layout_height="44dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_login_bg_style"
        android:gravity="center"
        android:text="@string/complete"
        android:textColor="@color/iff_FFFFFF"
        android:textSize="24sp" />
</LinearLayout>
